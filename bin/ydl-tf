WORKERS=0
PSES=0
JAR=""
while true
do
    case "$1" in
         -num_worker)
            WORKERS="$2" 
            echo "worker num: $WORKERS"
            shift  
            ;;  
         -num_ps)
            PSES="$2" 
            echo "ps num: $PSES"
            shift  
            ;;  
         -jar)
            JAR="$2"
            echo "jar path: $JAR"
            shift
            ;;
         *)
            shift  
         break 
         ;;
    esac
shift
done


yarn jar $JAR
         --num_worker $WORKERS \
         --num_ps $PSES \
         --container_memory 1024 \
         --tf_lib "../samples/between-graph/libbridge.so"
